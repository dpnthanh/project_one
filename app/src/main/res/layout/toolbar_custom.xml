<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.Toolbar
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="65dp"
    android:background="@drawable/background_toobar_mydashboard">
    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <ImageView
            android:src="@drawable/icon_menu_header_mydrashboard"
            android:scaleType="fitCenter"
            android:adjustViewBounds="true"
            android:layout_margin="5dp"
            android:layout_width="35dp"
            android:layout_height="wrap_content" />
        <ImageView
            android:src="@drawable/icon_new_notification_toolbar_main"
            android:scaleType="fitCenter"
            android:layout_gravity="top|right"
            android:adjustViewBounds="true"
            android:layout_width="15dp"
            android:layout_height="15dp" />

    </FrameLayout>


    <TextView
        android:text="@string/textView_header_mydashboard"
        android:textColor="#fff"
        android:textSize="20sp"
        android:layout_gravity="center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <ImageView
        android:src="@drawable/icon_help_header_mydashboard"
        android:scaleType="fitXY"
        android:adjustViewBounds="true"
        android:layout_gravity="right"
        android:layout_marginRight="10dp"
        android:layout_width="35sp"
        android:layout_height="wrap_content" />

</android.support.v7.widget.Toolbar>